{
  "extends": [
    "@rocketseat/eslint-config/react"
  ],
  "rules": {
    "camelcase": "off",
    "@typescript-eslint/camelcase": "off",
    "no-unused-vars": [
      "warn",
      {
        "vars": "all",
        "args": "after-used",
        "ignoreRestSiblings": false
      }
    ],
    "@typescript-eslint/no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }]
  }
}